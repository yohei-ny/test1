<h1>Editing Book</h1>
<%= form_with model:@book, url:"/books/#{@book.id}", local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :Title %>
  <h4>Body</h4>
  <%= f.text_area :Body %>
  <%= f.submit 'Update Book' %>
  <%= link_to "Edit", edit_book_path(@book.id) %>
  <%= link_to "Back", index_book_path(@book.id) %>

<% end %>